<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:FlowPanel styleName="bk-Logout bk-Form">
		<g:HTMLPanel styleName="fields">
			<h1>Cerrar sesión</h1>
			<p>
				<label>Estás registrado como:</label>
				<g:Label ui:field="user" />
			</p>
		</g:HTMLPanel>
		<g:FlowPanel styleName="actions">
			<g:Anchor ui:field="logout">Salir</g:Anchor>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 