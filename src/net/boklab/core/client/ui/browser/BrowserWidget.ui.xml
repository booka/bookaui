<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
    <g:LayoutPanel styleName="bk-Browser">
        <g:layer left="0" top="0" right="0" height="2em">
            <g:Label ui:field="title" styleName="browserName"></g:Label>
        </g:layer>
        <g:layer left="3px" top="2em" right="3px" bottom="33px">
            <g:ScrollPanel styleName="Scroll">
                <g:FlowPanel ui:field="list" styleName="list"></g:FlowPanel>
            </g:ScrollPanel>
        </g:layer>
        <g:layer left="3px" bottom="0" right="0" height="30px">
            <g:FlowPanel ui:field="actions" styleName="actions">
            </g:FlowPanel>
        </g:layer>
    </g:LayoutPanel>
</ui:UiBinder> 