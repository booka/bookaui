<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.container {
			width: 100%;
			height: 100%;
			display: block;
			overflow: hidden;
		}
	</ui:style>
	<g:FlowPanel styleName="{style.container}">
	<g:FlowPanel styleName="bk-Calendar">
	<g:Label styleName="year">2010</g:Label>
	<g:SimplePanel ui:field="content" styleName="calendar"></g:SimplePanel>
	</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 