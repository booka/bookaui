<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
    <g:ScrollPanel styleName="bk-Document">
        <g:FlowPanel styleName="Page" ui:field="page">
            <g:FlowPanel ui:field="properties">
            <g:Label ui:field="title" styleName="title"></g:Label>
            <g:Label ui:field="description" styleName="description"></g:Label>
            </g:FlowPanel>
            <g:FlowPanel styleName="Content"
                ui:field="content"></g:FlowPanel>
        </g:FlowPanel>
    </g:ScrollPanel>
</ui:UiBinder> 