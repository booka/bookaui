<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.app {
			background-color: #C6C8CA;
		}
		
		.slide {
			background-color: #C6C8CA;
			height: 100%;
			width: 100%;
		}
		
		.top {
			background-color: white;
		}
	</ui:style>
	<g:LayoutPanel styleName="{style.app}">
		<g:layer left="0" top="0" right="0" height="80px">
			<g:SimplePanel ui:field="north" styleName="{style.top}">
			</g:SimplePanel>
		</g:layer>
		<g:layer left="0" top="81px" right="0px" bottom="0">
			<g:LayoutPanel ui:field="center" styleName="{style.slide}">
			</g:LayoutPanel>
		</g:layer>
        <g:layer  top="81px" right="0" width="0px" bottom="0">
            <g:SimplePanel ui:field="emite" styleName="{style.app}">
            </g:SimplePanel>
        </g:layer>
	</g:LayoutPanel>
</ui:UiBinder> 